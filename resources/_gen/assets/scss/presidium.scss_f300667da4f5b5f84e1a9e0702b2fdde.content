/* Triangle styles. */
.tooltips-term {
  position: relative;
  display: inline-block;
  border-bottom: 1px dashed #e49134;
  font-style: normal;
  cursor: help;
  color: #666;
  text-decoration: none; }
  .tooltips-term .tooltips-text {
    visibility: collapse;
    position: absolute;
    opacity: 0;
    height: auto;
    max-height: 276px;
    width: 276px;
    font-style: normal;
    background: #fff;
    -webkit-box-shadow: 0px 0px 5px 5px #dadada;
    -moz-box-shadow: 0px 0px 5px 5px #dadada;
    box-shadow: 0px 0px 5px 5px #dadada;
    color: #666;
    text-align: left;
    border-radius: 6px;
    bottom: 120%;
    padding: 5px;
    margin-left: -165px;
    z-index: 1060;
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
    -ms-transition: all 0.3s ease-in-out;
    -o-transition: all 0.3s ease-in-out;
    transition: all 0.3s ease-in-out; }
    .tooltips-term .tooltips-text p {
      padding: 10px; }
  .tooltips-term:hover .tooltips-text {
    transition-delay: 1s;
    -webkit-transition-delay: 1s;
    -ms-transition-delay: 1s;
    -o-transition-delay: 1s;
    visibility: visible;
    overflow-y: auto;
    opacity: 1; }
  .tooltips-term:hover {
    color: #666;
    text-decoration: none; }

.tooltips-term::after {
  content: '';
  position: absolute;
  height: 0;
  width: 0;
  top: -20%;
  left: 50%;
  z-index: 1060;
  margin-left: -8px;
  border-top: 8px solid #FFFFFF;
  border-right: 8px solid transparent;
  border-left: 8px solid transparent; }

